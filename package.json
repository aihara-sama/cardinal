    "@prisma/client": "^3.6.0",
    "@quixo3/prisma-session-store": "^3.1.2",
    "@types/bcrypt": "^5.0.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
